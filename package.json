  "type": "commonjs",
  "exports": {
    "require": "./rollup-plugin-keep-import.js",
    "import": "./rollup-plugin-keep-import.mjs"
  },
  "main": "rollup-plugin-keep-import.js",
  "types": "rollup-plugin-keep-import.d.ts",
  "files": [
    "rollup-plugin-keep-import.js",
    "rollup-plugin-keep-import.mjs",
    "rollup-plugin-keep-import.d.ts"
  ],
  "scripts": {
    "cleanup": "yarn format",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\""
  },
  "prettier": "@willbooster/prettier-config",
  "resolutions": {
    "npm/chalk": "^4.1.2"
  },
  "devDependencies": {
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "pinst": "3.0.0",
    "prettier": "2.8.1",
    "rollup": "3.9.1",
    "semantic-release": "19.0.5",
    "sort-package-json": "2.1.0"
  },
  "packageManager": "yarn@4.0.0-rc.34",
  "publishConfig": {
    "access": "public"
  }
